%tr
  %td
    = link_to 'Edit', edit_resource_url(location), class: 'btn btn-warning btn-xs'
    &nbsp;
    = link_to "Delete", resource_url(location), :method => :delete, class: "delete btn btn-danger btn-xs", confirm: "Are you sure ?"
  %td= location.id
  %td= location.located_at
  %td= location.lat
  %td= location.lng
  %td= location.reverse
